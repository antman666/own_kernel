pkgname=ananicy-rules
_gitname=ananicy-rules
pkgver=1
pkgrel=1
arch=('any')
license=('GPL')
pkgdesc='CachyOS - ananicy-rules '
makedepends=('git')
source=("git+https://github.com/antman666/$_gitname")
sha256sums=('SKIP')

package() {
  cd $gitname
  install -d $pkgdir/etc/ananicy.d
  cp -rf $srcdir/$gitname/* $pkgdir/etc/ananicy.d
}
